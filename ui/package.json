  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/Bhamni/opd/issues"
  },
  "jest": {
    "roots": [
      "<rootDir>/react-components"
    ],
    "collectCoverageFrom": [
      "react-components/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/react-components/**/*.{spec,test}.{js,jsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "\\.(css|less)$": "<rootDir>/react-components/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
