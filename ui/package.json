  "eslintConfig": {
    "extends":["react-app", "plugin:jsx-a11y/recommended"],
    "plugins": ["jsx-a11y"]
  },
  "scripts": {
    "test": "karma start test/config/karma.conf.js",
    "test-react": "TZ=UTC jest",
    "test-react-watch": "TZ=UTC jest --watch",
    "build-react": "webpack --env=production -p",
    "build-react-dev": "webpack --env=developemnt --watch",
    "storybook": "start-storybook",
    "lint": "eslint --ext .js --ext .jsx react-components/",
    "lint-fix": "eslint  --ext .js --ext .jsx react-components/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Bhamni/opd.git"
  },
  "author": "bahmni@thoughtworks.com",
  "license": "Apache-2.0",
